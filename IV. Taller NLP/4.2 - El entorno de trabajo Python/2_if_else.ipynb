    "# Pedir un número al usuario\n",
    "numero = int(input(\"Ingresa un número: \"))\n",
    "\n",
    "# Evaluar si es positivo, negativo o cero\n",
    "if numero > 0:\n",
    "    print(\"El número es positivo.\")\n",
    "elif numero < 0:\n",
    "    print(\"El número es negativo.\")\n",
    "else:\n",
    "    print(\"El número es cero.\")"
   ]
